<h1>Listing skill_profiles</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Businessinfo</th>
    <th>Primarytel</th>
    <th>Mobile</th>
    <th>Workradius</th>
    <th>Emailalerts</th>
    <th>Address</th>
    <th>Subscriber</th>
    <th>Skillcategory</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @skill_profiles.each do |skill_profile| %>
  <tr>
    <td><%= skill_profile.Name %></td>
    <td><%= skill_profile.BusinessInfo %></td>
    <td><%= skill_profile.PrimaryTel %></td>
    <td><%= skill_profile.Mobile %></td>
    <td><%= skill_profile.WorkRadius %></td>
    <td><%= skill_profile.EmailAlerts %></td>
    <td><%= skill_profile.Address_id %></td>
    <td><%= skill_profile.Subscriber_id %></td>
    <td><%= skill_profile.SkillCategory_id %></td>
    <td><%= link_to 'Show', skill_profile %></td>
    <td><%= link_to 'Edit', edit_skill_profile_path(skill_profile) %></td>
    <td><%= link_to 'Destroy', skill_profile, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

